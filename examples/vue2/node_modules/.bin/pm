#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/pm-cli/bin/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/pm-cli/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/node_modules:/Users/jinpengh/Desktop/jinpengh/project/node_modules:/Users/jinpengh/Desktop/jinpengh/node_modules:/Users/jinpengh/Desktop/node_modules:/Users/jinpengh/node_modules:/Users/node_modules:/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/pm-cli/bin/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/pm-cli/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/packages/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/node_modules:/Users/jinpengh/Desktop/jinpengh/project/node_modules:/Users/jinpengh/Desktop/jinpengh/node_modules:/Users/jinpengh/Desktop/node_modules:/Users/jinpengh/node_modules:/Users/node_modules:/node_modules:/Users/jinpengh/Desktop/jinpengh/project/personal-project/pm-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/pm-cli/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../packages/pm-cli/bin/index.js" "$@"
fi
